<!DOCTYPE html>
<html>
<head>
  <title>Admin panel</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "admin/all" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<section>
		<% flash.each do |key, value| %>
			<%= content_tag(:div, value, :class => "flash #{key}") %>
			<!-- <div class="flash <%= key %>"><%= value %></div> <!-->
		<% end %>
		<%= yield %>
	</section>
</body>
</html>
